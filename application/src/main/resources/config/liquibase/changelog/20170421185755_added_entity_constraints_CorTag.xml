<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CorTag.
    -->
    <changeSet id="20170421185755-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="channel_id"
                                 baseTableName="cor_tag"
                                 constraintName="fk_cor_tag_channel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_channel"/>

        <addForeignKeyConstraint baseColumnNames="tags_id"
                                 baseTableName="cor_tag"
                                 constraintName="fk_cor_tag_tags_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_media_item"/>

    </changeSet>
</databaseChangeLog>
