<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Clock.
    -->
    <changeSet id="20170720190703-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="grid_id"
                                 baseTableName="sch_clock"
                                 constraintName="fk_clock_grid_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_grid"/>

        <addForeignKeyConstraint baseColumnNames="queue_params_id"
                                 baseTableName="sch_clock"
                                 constraintName="fk_clock_queue_params_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_queue_params"/>

        <addForeignKeyConstraint baseColumnNames="time_params_id"
                                 baseTableName="sch_clock"
                                 constraintName="fk_clock_time_params_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_time_params"/>
    
        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="sch_clock"
                                 constraintName="fk_sch_clock_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="channel_id"
                                 baseTableName="sch_clock"
                                 constraintName="fk_sch_clock_channel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_channel"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="sch_clock"
                                 constraintName="fk_sch_clock_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>

        <addForeignKeyConstraint baseColumnNames="last_modified_by_id"
                                 baseTableName="sch_clock"
                                 constraintName="fk_sch_clock_last_modified_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>
    </changeSet>
</databaseChangeLog>
