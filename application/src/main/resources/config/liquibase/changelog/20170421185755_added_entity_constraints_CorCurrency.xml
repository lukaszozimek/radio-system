<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CorCurrency.
    -->
    <changeSet id="20170421185755-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="organization_id"
                                 baseTableName="cor_currency"
                                 constraintName="fk_cor_currency_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_organization"/>

        <addForeignKeyConstraint baseColumnNames="last_modified_by_id"
                                 baseTableName="cor_currency"
                                 constraintName="fk_cor_currency_last_modified_by"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="cor_currency"
                                 constraintName="fk_cor_currency_created_by"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>
    </changeSet>
</databaseChangeLog>
