<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Emission.
    -->
    <changeSet id="20170720190706-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="playlist_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_emission_playlist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_playlist"/>

        <addForeignKeyConstraint baseColumnNames="clock_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_emission_clock_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_clock"/>

        <addForeignKeyConstraint baseColumnNames="media_item_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_emission_media_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_media_item"/>

        <addForeignKeyConstraint baseColumnNames="queue_params_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_emission_queue_params_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_queue_params"/>

        <addForeignKeyConstraint baseColumnNames="time_params_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_emission_time_params_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_time_params"/>

        <addForeignKeyConstraint baseColumnNames="block_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_emission_block_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_block"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_sch_emission_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="channel_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_sch_emission_channel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_channel"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_sch_emission_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>

        <addForeignKeyConstraint baseColumnNames="last_modified_by_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_sch_emission_last_modified_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>

    </changeSet>
</databaseChangeLog>
