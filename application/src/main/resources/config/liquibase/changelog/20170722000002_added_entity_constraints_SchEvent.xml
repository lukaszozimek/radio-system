<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Block.
    -->
    <changeSet id="20170720190704-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="queue_params_id"
                                 baseTableName="sch_block"
                                 constraintName="fk_block_queue_params_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_queue_params"/>

        <addForeignKeyConstraint baseColumnNames="time_params_id"
                                 baseTableName="sch_block"
                                 constraintName="fk_block_time_params_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_time_params"/>

    </changeSet>
</databaseChangeLog>
