<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity LibImageItem.
    -->
    <changeSet id="20170421185755-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="library_id"
                                 baseTableName="lib_image_item"
                                 constraintName="fk_lib_image_item_library_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_library"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="lib_image_item"
                                 constraintName="fk_lib_image_item_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="lib_image_item"
                                 constraintName="fk_lib_image_item_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>

        <addForeignKeyConstraint baseColumnNames="last_modified_by_id"
                                 baseTableName="lib_image_item"
                                 constraintName="fk_lib_image_item_modified_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>
    </changeSet>
</databaseChangeLog>
