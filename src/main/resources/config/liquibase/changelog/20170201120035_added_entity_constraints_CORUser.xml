<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CORUser.
    -->
    <changeSet id="20170201120035-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cornetwork_id"
                                 baseTableName="cor_user"
                                 constraintName="fk_coruser_cornetwork_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="corchannel_id"
                                 baseTableName="cor_user"
                                 constraintName="fk_coruser_corchannel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_channel"/>

        <addForeignKeyConstraint baseColumnNames="lib_library_id"
                                 baseTableName="cor_user"
                                 constraintName="fk_coruser_lib_library_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_library"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="cor_user"
                                 constraintName="fk_coruser_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

    </changeSet>
</databaseChangeLog>
