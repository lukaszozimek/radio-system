<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity SCHEmission.
    -->
    <changeSet id="20170111193429-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="block_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_block_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_block"/>

        <addForeignKeyConstraint baseColumnNames="media_item_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_media_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_media_item"/>

        <addForeignKeyConstraint baseColumnNames="template_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_template_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_template"/>
        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

    </changeSet>
</databaseChangeLog>
