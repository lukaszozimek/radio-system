<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity TRACustomer.
    -->
    <changeSet id="20170111193433-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="industry_id"
                                 baseTableName="tra_customer"
                                 constraintName="fk_tracustomer_industry_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tra_industry"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="tra_customer"
                                 constraintName="fk_tracustomer_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="account_id"
                                 baseTableName="tra_customer"
                                 constraintName="fk_tracustomer_account_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
