<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CORTag.
    -->
    <changeSet id="20170201120003-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="cor_tag"
                                 constraintName="fk_cortag_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="libmedia_item_id"
                                 baseTableName="cor_tag"
                                 constraintName="fk_cortag_libmedia_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_media_item"/>

    </changeSet>
</databaseChangeLog>
