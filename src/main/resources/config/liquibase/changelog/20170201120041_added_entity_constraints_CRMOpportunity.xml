<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CRMOpportunity.
    -->
    <changeSet id="20170201120041-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="stage_id"
                                 baseTableName="crm_opportunity"
                                 constraintName="fk_crmopportunity_stage_id"
                                 referencedColumnNames="id"
                                 referencedTableName="crm_stage"/>

        <addForeignKeyConstraint baseColumnNames="keeper_id"
                                 baseTableName="crm_opportunity"
                                 constraintName="fk_crmopportunity_keeper_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_user"/>

        <addForeignKeyConstraint baseColumnNames="contact_id"
                                 baseTableName="crm_opportunity"
                                 constraintName="fk_crmopportunity_contact_id"
                                 referencedColumnNames="id"
                                 referencedTableName="crm_contact"/>

        <addForeignKeyConstraint baseColumnNames="account_id"
                                 baseTableName="crm_opportunity"
                                 constraintName="fk_crmopportunity_account_id"
                                 referencedColumnNames="id"
                                 referencedTableName="crm_account"/>

        <addForeignKeyConstraint baseColumnNames="lead_id"
                                 baseTableName="crm_opportunity"
                                 constraintName="fk_crmopportunity_lead_id"
                                 referencedColumnNames="id"
                                 referencedTableName="crm_lead"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="crm_opportunity"
                                 constraintName="fk_crmopportunity_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="crmaccount_id"
                                 baseTableName="crm_opportunity"
                                 constraintName="fk_crmopportunity_crmaccount_id"
                                 referencedColumnNames="id"
                                 referencedTableName="crm_account"/>

    </changeSet>
</databaseChangeLog>
