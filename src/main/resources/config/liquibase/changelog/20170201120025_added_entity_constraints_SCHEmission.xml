<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity SCHEmission.
    -->
    <changeSet id="20170201120025-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="schblock_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_schblock_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sch_block"/>

        <addForeignKeyConstraint baseColumnNames="media_item_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_media_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_media_item"/>

        <addForeignKeyConstraint baseColumnNames="channel_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_channel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_channel"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="tracampaign_id"
                                 baseTableName="sch_emission"
                                 constraintName="fk_schemission_tracampaign_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tra_campaign"/>

    </changeSet>
</databaseChangeLog>
