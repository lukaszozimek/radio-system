<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity LIBMediaItem.
    -->
    <changeSet id="20170201120017-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="lib_library_id"
                                 baseTableName="lib_media_item"
                                 constraintName="fk_libmedia_item_lib_library_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_library"/>

        <addForeignKeyConstraint baseColumnNames="album_id"
                                 baseTableName="lib_media_item"
                                 constraintName="fk_libmedia_item_album_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_album"/>

        <addForeignKeyConstraint baseColumnNames="artist_id"
                                 baseTableName="lib_media_item"
                                 constraintName="fk_libmedia_item_artist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_artist"/>

        <addForeignKeyConstraint baseColumnNames="composer_id"
                                 baseTableName="lib_media_item"
                                 constraintName="fk_libmedia_item_composer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_label"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="lib_media_item"
                                 constraintName="fk_libmedia_item_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="lib_media_item"
                                 constraintName="fk_libmedia_item_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_library"/>

    </changeSet>
</databaseChangeLog>
