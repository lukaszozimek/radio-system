<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CORPropertyValue.
    -->
    <changeSet id="20170201120002-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="corproperty_key_id"
                                 baseTableName="cor_property_value"
                                 constraintName="fk_corproperty_value_corproperty_key_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_property_key"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="cor_property_value"
                                 constraintName="fk_corproperty_value_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

    </changeSet>
</databaseChangeLog>
