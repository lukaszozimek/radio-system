<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity LIBFileItem.
    -->
    <changeSet id="20170201120019-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="lib_file_item"
                                 constraintName="fk_libfile_item_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="parent_file_id"
                                 baseTableName="lib_file_item"
                                 constraintName="fk_libfile_item_parent_file_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_file_item"/>

    </changeSet>
</databaseChangeLog>
