        <addForeignKeyConstraint baseColumnNames="channel_id"
                                 baseTableName="cfg_external_system_log"
                                 constraintName="fk_cfg_external_system_log_channel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_channel"/>

    </changeSet>
</databaseChangeLog>
