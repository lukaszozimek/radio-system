<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CfgExternalSystemLog.
    -->
    <changeSet id="20170321202155-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="cfg_external_system_log"
                                 constraintName="fk_cfg_external_system_log_network_id"
                                 referencedColumnNames="id"
