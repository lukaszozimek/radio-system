<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity TRAAdvertisement.
    -->
    <changeSet id="20170201120027-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="crmaccount_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_crmaccount_id"
                                 referencedColumnNames="id"
                                 referencedTableName="crm_account"/>

        <addForeignKeyConstraint baseColumnNames="libitem_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_libitem_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_media_item"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="crm_account"/>

        <addForeignKeyConstraint baseColumnNames="industry_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_industry_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tra_industry"/>

        <addForeignKeyConstraint baseColumnNames="network_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_network_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor_network"/>

        <addForeignKeyConstraint baseColumnNames="type_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tra_advertisment_type"/>

    </changeSet>
</databaseChangeLog>
