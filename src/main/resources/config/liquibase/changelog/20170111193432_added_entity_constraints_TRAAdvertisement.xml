<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity TRAAdvertisement.
    -->
    <changeSet id="20170111193432-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="media_item_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_media_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lib_media_item"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tra_customer"/>

        <addForeignKeyConstraint baseColumnNames="industry_id"
                                 baseTableName="tra_advertisement"
                                 constraintName="fk_traadvertisement_industry_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tra_industry"/>

    </changeSet>
</databaseChangeLog>
