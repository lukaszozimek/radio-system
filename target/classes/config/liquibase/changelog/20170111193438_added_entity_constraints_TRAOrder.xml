<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity TRAOrder.
    -->
    <changeSet id="20170111193438-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="campaign_id"
                                 baseTableName="tra_order"
                                 constraintName="fk_traorder_campaign_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tra_campaign"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="tra_order"
                                 constraintName="fk_traorder_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tra_customer"/>

    </changeSet>
</databaseChangeLog>
